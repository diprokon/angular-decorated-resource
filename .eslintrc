{
    "rules": {
        "quotes": [
            2,
            "single",
            {
                "allowTemplateLiterals": true
            }
        ],
        "angular/log": [
            1
        ],
        "no-console": [
            1
        ]
    },
    "plugins": [
        "angular",
        "jasmine"
    ],
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "jasmine": true
    },
    "globals": {
        "angular": true,
        "inject": true
    }
}
